{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Debug"
      },
      "android": {
        "resourceClass": "medium",
        "withoutCredentials": true,
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "EXPO_PUBLIC_BLE_ENABLED": "true",
        "EXPO_PUBLIC_BLE_DEBUG": "true",
        "EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_SOUND_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_VIBRATION_ENABLED": "true"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_SOUND_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_VIBRATION_ENABLED": "true"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Release",
        "autoIncrement": "buildNumber"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "app-bundle",
        "autoIncrement": "versionCode"
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_BLE_ENABLED": "true",
        "EXPO_PUBLIC_BLE_DEBUG": "false",
        "EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_SOUND_ENABLED": "true",
        "EXPO_PUBLIC_NOTIFICATION_VIBRATION_ENABLED": "true"
      }
    },
    "production-ble": {
      "extends": "production",
      "ios": {
        "resourceClass": "m-large",
        "simulator": false,
        "buildConfiguration": "Release",
        "autoIncrement": "buildNumber"
      },
      "android": {
        "resourceClass": "large",
        "buildType": "app-bundle",
        "autoIncrement": "versionCode"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_BLE_ENABLED": "true",
        "EXPO_PUBLIC_BLE_DEBUG": "false",
        "EXPO_PUBLIC_BLE_PERFORMANCE_MONITORING": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "praburks@yahoo.com",
        "appleTeamId": "7H5VHF43BT"
      },
      "android": {
        "serviceAccountKeyPath": "../path/to/api-key.json",
        "track": "internal"
      }
    }
  }
}